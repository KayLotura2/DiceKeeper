{
  "ancestries-schema": {
    "id": "#ancestries",
    "title": "Ancestries",
    "description": "A collection of valid NPC ancestries",
    "type": "array",
    "items": {
      "$ref": "#/definitions/ancestry"
    },
    "definitions": {
      "ancestry": {
        "id": "#ancestry",
        "title": "Ancestry",
        "description": "A detailed description of a NPC's ancestry",
        "type": "object",
        "properties": {
          "name": {
            "title": "Base Ancestry Name",
            "description": "The base ancestry's name",
            "type": "string"
          },
          "types": {
            "title": "Types",
            "description": "The base types of the NPC",
            "type": "array",
            "items": {
              "$ref": "#/type"
            }
          },
          "subtypes": {
            "title": "Subtypes",
            "description": "The base subtypes of the NPC",
            "type": "array",
            "items": {
              "$ref": "#/subType"
            }
          },
          "baseSize": {
            "$ref": "#size"
          },
          "baseMod": {
            "title": "Base Abiliy Modifier",
            "description": "The base ability modifier of the NPC's ancestry",
            "type": "object",
            "properties": {
              "ability": {
                "$ref": "#ability"
              },
              "bonus": {
                "type": "number",
                "enum": [
                  0,
                  1,
                  2
                ]
              }
            }
          },
          "senses": {
            "title": "Senses",
            "description": "The base senses the NPC has for their ancestry",
            "type": "array",
            "items": {
              "senseObj": {
                "properties": {
                  "sense": {
                    "$ref": "#sense"
                  },
                  "distance": {
                    "type": "number"
                  }
                },
                "required": [
                  "sense",
                  "distance"
                ]
              }
            }
          },
          "moveSpeeds": {
            "title": "Move Speeds",
            "description": "The base move speeds the NPC has for their ancestry",
            "type": "array",
            "items": {
              "moveSpeedObj": {
                "properties": {
                  "movetype": {
                    "$ref": "#moveType"
                  },
                  "distance": {
                    "type": "number"
                  }
                },
                "required": [
                  "movetype",
                  "distance"
                ]
              }
            }
          },
          "resistances": {
            "title": "Resistances",
            "description": "The base resistances the NPC has for their ancestry",
            "type": "array",
            "items": {
              "$ref": "#resistance"
            }
          },
          "naturealWeapons": {
            "title": "Natural Weapons",
            "description": "The natural wreapons provided by the base ancestry",
            "type": "array",
            "items": {
              "$ref": "#naturalWeapon"
            }
          },
          "weaponProfs": {
            "title": "Weapon Proficiencies",
            "description": "Weapon Proficiencies provided by the base ancestry",
            "type": "array",
            "items": {
              "$ref": "#weapon"
            }
          },
          "skillProficiencies": {
            "title": "Proficiencies",
            "description": "The skill and tool proficiencies of the NPC's sub ancestry",
            "type": "array",
            "items": {
              "ref": "#skill"
            }
          },
          "languages": {
            "title": "Languages",
            "description": "Languages granted by baseAncestry",
            "type": "array",
            "items": {
              "ref": "#language"
            }
          },
          "other": {
            "title": "Other Abilities",
            "description": "Strings that may be further implimented later, Special Abilities",
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "subs": {
            "title": "Sub-Ancestries",
            "description": "The sub-ancestries available for the given base ancestry",
            "type": "array",
            "items": {
              "$ref": "#/definitions/subAncestry"
            }
          }
        },
        "definitions": {
          "subAncestry": {
            "id": "#subAncestry",
            "title": "Sub-Ancestry",
            "description": "A sub-ancestry's values for the given base ancestry",
            "type": "object",
            "properties": {
              "name": {
                "title": "Sub-Ancestry Name",
                "description": "A sub-ancestry's name, usually a modifer to the base ancestry",
                "type": "string"
              },
              "size": {
                "title": "Sub-Ancestry Size",
                "description": "A sub-ancestry's size.",
                "type": {
                  "$ref": "#size"
                }
              },
              "subMod": {
                "title": "The Subraces Ability Modifier",
                "description": "The ability modifier of the NPC's sub ancestry",
                "type": "object",
                "properties": {
                  "ability": {
                    "$ref": "#ability"
                  },
                  "bonus": {
                    "type": "number",
                    "enum": [
                      0,
                      1,
                      2
                    ]
                  }
                },
                "skillProficiencies": {
                  "title": "Proficiencies",
                  "description": "The skill and tool proficiencies of the NPC's sub ancestry",
                  "type": "array",
                  "items": {
                    "ref": "#skill"
                  }
                },
                "languages": {
                  "title": "Languages",
                  "description": "Languages granted by subAncestry",
                  "type": "array",
                  "items": {
                    "ref": "#language"
                  }
                },
                "senses": {
                  "title": "Senses",
                  "description": "The senses the NPC has for their subAncestry",
                  "type": "array",
                  "items": {
                    "senseObj": {
                      "properties": {
                        "sense": {
                          "$ref": "#sense"
                        },
                        "distance": {
                          "type": "number"
                        }
                      },
                      "required": [
                        "sense",
                        "distance"
                      ]
                    }
                  }
                },
                "moveSpeeds": {
                  "title": "Move Speeds",
                  "description": "The  move speeds the NPC has for their subAncestry",
                  "type": "array",
                  "items": {
                    "moveSpeedObj": {
                      "properties": {
                        "movetype": {
                          "$ref": "#moveType"
                        },
                        "distance": {
                          "type": "number"
                        }
                      },
                      "required": [
                        "movetype",
                        "distance"
                      ]
                    }
                  }
                },
                "other": {
                  "title": "Other Abilities",
                  "description": "Strings that may be further implimented later, Special Abilities",
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                }
              },
              "required": [
                "name",
                "subMod"
              ]
            }
          },
          "required": [
            "rt",
            "types",
            "baseSize",
            "baseMod",
            "moveSpeeds",
            "subs"
          ]
        }
      }
    },
