{
  "compilerOptions": {
    /* Basic Options */
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "lib": [
      "es2015",
      "dom"
    ],
    "sourceMap": true,
    "strict": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}